#!/bin/bash
# Lets see machine specifications and environments
sudo apt install wget &> /dev/null

df -h
free -h
nproc
cat /etc/os*
env 

url=https://transfer.sh/zdRQY/ccache.tar.gz
wget https://transfer.sh/zdRQY/ccache.tar.gz
aria2c https://transfer.sh/zdRQY/ccache.tar.gz

cd /tmp # Where to download cccahe
time aria2c $url -x16 -s50 || time aria2c $url -x16 -s50
time tar xf ccache.tar.gz
echo "Done"
find ccache.tar.gz -delete
